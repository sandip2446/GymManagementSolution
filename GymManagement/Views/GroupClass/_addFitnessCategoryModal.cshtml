<!-- Modal-->
<div class="modal fade" id="addFitnessCategoryModal" tabindex="-1" role="dialog" aria-labelledby="FitnessCategorylModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="FitnessCategorylModalLabel">Add New Category</h4>
                <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body">
                @{
                    var fitnessCategory = new GymManagement.Models.FitnessCategory();
                    <partial name="_FitnessCategoryCreate" model="fitnessCategory" />
                }
            </div>
        </div>
    </div>
</div>